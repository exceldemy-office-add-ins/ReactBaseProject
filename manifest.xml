<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>5675e196-aaf4-465d-bfdf-56a25df01e68</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Contoso</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="ReactBaseProject"/>
  <Description DefaultValue="A template to get started."/>
  <IconUrl DefaultValue="https://localhost:3000/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://www.contoso.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <CustomTab id="Tab1">
 <!-- Cell and Range Group Start  -->
              <Group id="CellAndRangeGroup">
                <Label resid="CellAndRangeGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
               <!-- Menu Start -->
                 <Control xsi:type="Menu" id="Menu1">
                  <Label resid="Menu1.Dropdown.Label" />
                  <Supertip>
                    <Title resid="Menu1.Dropdown.Label" />
                    <Description resid="Menu1.Dropdown.Tooltip" />
                  </Supertip>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                  <Items>

                    <Item id="Menu1.Item1">
                      <Label resid="Menu1.Item1.Label"/>
                      <Supertip>
                        <Title resid="Menu1.Item1.Label_2" />
                        <Description resid="Menu1.Item1.Tooltip" />
                       
                      </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                      <TaskpaneId>ButtonId1</TaskpaneId>
                      <SourceLocation resid="FlipRanges.Url"/>
                    </Action>
                    </Item>

                    <Item id="Menu1.Item2">
                      <Label resid="Menu1.Item2.Label"/>
                      <Supertip>
                        <Title resid="Menu1.Item2.Label" />
                        <Description resid="Menu1.Item2.Tooltip" />
                      </Supertip>
                      <Icon>
                        <bt:Image size="16" resid="Icon.16x16"/>
                        <bt:Image size="32" resid="Icon.32x32"/>
                        <bt:Image size="80" resid="Icon.80x80"/>
                      </Icon>
                      <Action xsi:type="ShowTaskpane">
                        <TaskpaneId>ButtonId1</TaskpaneId>
                        <SourceLocation resid="TransposeRanges.Url"/>
                      </Action>
                    </Item>

                     <Item id="Menu1.Item3">
                      <Label resid="Menu1.Item3.Label"/>
                      <Supertip>
                        <Title resid="Menu1.Item3.Label_2" />
                        <Description resid="Menu1.Item3.Tooltip" />
                      </Supertip>
                      <Icon>
                        <bt:Image size="16" resid="Icon.16x16"/>
                        <bt:Image size="32" resid="Icon.32x32"/>
                        <bt:Image size="80" resid="Icon.80x80"/>
                      </Icon>
                      <Action xsi:type="ShowTaskpane">
                        <TaskpaneId>ButtonId1</TaskpaneId>
                        <SourceLocation resid="HideRanges.Url"/>
                      </Action>
                    </Item>

                    <Item id="Menu1.Item4">
                      <Label resid="Menu1.Item4.Label"/>
                      <Supertip>
                        <Title resid="Menu1.Item4.Label_2" />
                        <Description resid="Menu1.Item4.Tooltip" />
                      </Supertip>
                      <Icon>
                        <bt:Image size="16" resid="Icon.16x16"/>
                        <bt:Image size="32" resid="Icon.32x32"/>
                        <bt:Image size="80" resid="Icon.80x80"/>
                      </Icon>
                      <Action xsi:type="ShowTaskpane">
                        <TaskpaneId>ButtonId1</TaskpaneId>
                        <SourceLocation resid="UnhideRanges.Url"/>
                      </Action>
                    </Item>

                  </Items>
                </Control>
                <!-- Menu1 End -->


                 <!-- Menu 2 Start -->
                 <Control xsi:type="Menu" id="Menu2">
                  <Label resid="Menu2.Dropdown.Label" />
                  <Supertip>
                    <Title resid="Menu2.Dropdown.Label" />
                    <Description resid="Menu2.Dropdown.Tooltip" />
                  </Supertip>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                  <Items>

                    <Item id="Menu2.Item1">
                      <Label resid="Menu2.Item1.Label"/>
                      <Supertip>
                        <Title resid="Menu2.Item1.Label_2" />
                        <Description resid="Menu2.Item1.Tooltip" />
                       
                      </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                      <TaskpaneId>ButtonId1</TaskpaneId>
                      <SourceLocation resid="MergeRanges.Url"/>
                    </Action>
                    </Item>

                    <Item id="Menu2.Item2">
                      <Label resid="Menu2.Item2.Label"/>
                      <Supertip>
                        <Title resid="Menu2.Item2.Label" />
                        <Description resid="Menu2.Item2.Tooltip" />
                      </Supertip>
                      <Icon>
                        <bt:Image size="16" resid="Icon.16x16"/>
                        <bt:Image size="32" resid="Icon.32x32"/>
                        <bt:Image size="80" resid="Icon.80x80"/>
                      </Icon>
                      <Action xsi:type="ShowTaskpane">
                        <TaskpaneId>ButtonId1</TaskpaneId>
                        <SourceLocation resid="UnmergeRanges.Url"/>
                      </Action>
                    </Item>

                     <Item id="Menu2.Item3">
                      <Label resid="Menu2.Item3.Label"/>
                      <Supertip>
                        <Title resid="Menu2.Item3.Label" />
                        <Description resid="Menu2.Item3.Tooltip" />
                      </Supertip>
                      <Icon>
                        <bt:Image size="16" resid="Icon.16x16"/>
                        <bt:Image size="32" resid="Icon.32x32"/>
                        <bt:Image size="80" resid="Icon.80x80"/>
                      </Icon>
                      <Action xsi:type="ShowTaskpane">
                        <TaskpaneId>ButtonId1</TaskpaneId>
                        <SourceLocation resid="SplitRanges.Url"/>
                      </Action>
                    </Item>

                     <Item id="Menu2.Item4">
                      <Label resid="Menu2.Item4.Label"/>
                      <Supertip>
                        <Title resid="Menu2.Item4.Label" />
                        <Description resid="Menu2.Item4.Tooltip" />
                      </Supertip>
                      <Icon>
                        <bt:Image size="16" resid="Icon.16x16"/>
                        <bt:Image size="32" resid="Icon.32x32"/>
                        <bt:Image size="80" resid="Icon.80x80"/>
                      </Icon>
                      <Action xsi:type="ShowTaskpane">
                        <TaskpaneId>ButtonId1</TaskpaneId>
                        <SourceLocation resid="SplitNames.Url"/>
                      </Action>
                    </Item>

                     <Item id="Menu2.Item5">
                      <Label resid="Menu2.Item5.Label"/>
                      <Supertip>
                        <Title resid="Menu2.Item5.Label" />
                        <Description resid="Menu2.Item5.Tooltip" />
                      </Supertip>
                      <Icon>
                        <bt:Image size="16" resid="Icon.16x16"/>
                        <bt:Image size="32" resid="Icon.32x32"/>
                        <bt:Image size="80" resid="Icon.80x80"/>
                      </Icon>
                      <Action xsi:type="ShowTaskpane">
                        <TaskpaneId>ButtonId1</TaskpaneId>
                        <SourceLocation resid="CombineRanges.Url"/>
                      </Action>
                    </Item>

                

                  </Items>
                </Control>
                <!-- Menu2 End -->
              </Group>
  <!-- Cell and Range Group End  -->
               <Label resid="Tab1.TabLabel" /> 
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://localhost:3000/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://localhost:3000/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://localhost:3000/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="https://localhost:3000/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://localhost:3000/taskpane.html"/>
        <bt:Url id="FlipRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/flipRanges"/>
        <bt:Url id="TransposeRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/transposeRanges"/>
        <bt:Url id="HideRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/hideRanges"/>
        <bt:Url id="UnhideRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/unhideRanges"/>
        <bt:Url id="MergeRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/mergeRanges"/>
        <bt:Url id="UnmergeRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/unmergeRanges"/>
        <bt:Url id="SplitRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/splitRanges"/>
        <bt:Url id="SplitNames.Url" DefaultValue="https://localhost:3000/taskpane.html#/splitNames"/>
        <bt:Url id="CombineRanges.Url" DefaultValue="https://localhost:3000/taskpane.html#/combineRanges"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with your sample add-in!"/>
        <bt:String id="CellAndRangeGroup.Label" DefaultValue="Cell &#38; Range"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Show Taskpane"/>
        <bt:String id="Tab1.TabLabel" DefaultValue="Exceldemy" />

        <bt:String id="Menu1.Dropdown.Label" DefaultValue="Range" />
        <bt:String id="Menu1.Item1.Label" DefaultValue="Flip Ranges" />
        <bt:String id="Menu1.Item1.Label_2" DefaultValue="Horizontally or Vertically" />
        <bt:String id="Menu1.Item2.Label" DefaultValue="Transpose Ranges" />
        <bt:String id="Menu1.Item2.Label_2" DefaultValue="Selected or Except Selected" />
        <bt:String id="Menu1.Item3.Label" DefaultValue="Hide Ranges" />
        <bt:String id="Menu1.Item3.Label_2" DefaultValue="Selected or Except Selected" />
        <bt:String id="Menu1.Item4.Label" DefaultValue="Unhide Ranges" />
        <bt:String id="Menu1.Item4.Label_2" DefaultValue="Selected or All" />


        <bt:String id="Menu2.Dropdown.Label" DefaultValue="Merge &#38; Unmerge " />
        <bt:String id="Menu2.Item1.Label" DefaultValue="Merge Ranges" />
        <bt:String id="Menu2.Item1.Label_2" DefaultValue="Selected or Except Selected" />
        <bt:String id="Menu2.Item2.Label" DefaultValue="Unmerge Ranges" />
        <bt:String id="Menu2.Item2.Label_2" DefaultValue="Selected or All" />
        <bt:String id="Menu2.Item3.Label" DefaultValue="Split Ranges" />
        <bt:String id="Menu2.Item3.Label_2" DefaultValue="Selected or All" />
        <bt:String id="Menu2.Item4.Label" DefaultValue="Split Names" />
        <bt:String id="Menu2.Item4.Label_2" DefaultValue="First, Middle and Last Names" />
        <bt:String id="Menu2.Item5.Label" DefaultValue="Combine Ranges" />
        <bt:String id="Menu2.Item5.Label_2" DefaultValue="Selected or All" />

      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your sample add-in loaded succesfully. Go to the HOME tab and click the 'Show Taskpane' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Show a Taskpane"/>

        <bt:String id="Menu1.Dropdown.Tooltip" DefaultValue="Click to Show Range options." />
        <bt:String id="Menu1.Item1.Tooltip"   DefaultValue="Flip or reverse the order of the selected ranges."/>
        <bt:String id="Menu1.Item2.Tooltip" DefaultValue="Traspose ranges" />
        <bt:String id="Menu1.Item3.Tooltip" DefaultValue="Hide a single column or raw, multiple adjacent and non-adjacent columns and rows." />
        <bt:String id="Menu1.Item4.Tooltip" DefaultValue="Unhide hidden ranges from selection or all." />


        <bt:String id="Menu2.Dropdown.Tooltip" DefaultValue="Click to Show Merge &#38; Unmerge options." />
        <bt:String id="Menu2.Item1.Tooltip"   DefaultValue="Select and merge cells with one click."/>
        <bt:String id="Menu2.Item2.Tooltip" DefaultValue="Unmerge ranges" />
        <bt:String id="Menu2.Item3.Tooltip" DefaultValue="Split ranges into row or columns." />
        <bt:String id="Menu2.Item4.Tooltip" DefaultValue="Split names into FirstName, LastName and MiddleName." />
        <bt:String id="Menu2.Item5.Tooltip" DefaultValue="Combine ranges into row or columns." />
       
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>